<div class="container">
  <div class="page-header">
    <h1>Login</h1>
    <%= form_tag login_path do %>
    <%= label_tag :user_name %>
    <%= text_field_tag :user_name %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag :login %>
    <% end %>
  </div>
</div>
